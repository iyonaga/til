{
  "name": "static-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "iyonaga <yiyonaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-serve",
    "build": "webpack --mode production",
    "lint": "yarn lint:js & yarn lint:scss",
    "lint:js": "eslint --ext .js,.jsx src --format codeframe",
    "lint:scss": "stylelint src/scss/**/*.scss",
    "lint:fix": "eslint --ext .js,.jsx src --format codeframe --fix",
    "format": "yarn format:js && yarn format:scss",
    "format:js": "prettier-eslint --write src/js/**/*.js",
    "format:scss": "prettier-stylelint --quiet --write src/scss/**/*.scss",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.scss": [
      "stylelint"
    ]
  },
  "prettier": {},
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "barba.js": "^1.0.0",
    "ress": "^1.2.2"
  }
}
