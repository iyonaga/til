{
  "name": "static-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "iyonaga <yiyonaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "lint": "yarn lint:js & yarn lint:scss",
    "lint:js": "eslint --ext .js,.jsx,.vue src --format codeframe",
    "lint:scss": "stylelint src/scss/**/*.scss",
    "lint:fix": "eslint --ext .js,.jsx,.vue src --format codeframe --fix",
    "format": "yarn format:js && yarn format:scss",
    "format:js": "prettier-eslint --write src/js/**/*.{js,vue}",
    "format:scss": "prettier-stylelint --quiet --write src/scss/**/*.scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.scss": [
      "stylelint"
    ]
  },
  "prettier": {},
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "ejs-templates-loader": "^0.0.3",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "barba.js": "^1.0.0",
    "ress": "^1.2.2",
    "vue": "^2.5.17"
  }
}
